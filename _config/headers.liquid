/*
  Access-Control-Allow-Origin: *
  Content-Security-Policy: default-src 'none' ; connect-src 'self' https://cloudflareinsights.com/ ; font-src data: https://fonts.googleapis.com https://fonts.gstatic.com ; img-src 'self' ; style-src 'self' https://fonts.googleapis.com ; script-src 'self' https://static.cloudflareinsights.com/beacon.min.js https://cdnjs.cloudflare.com;
