{
  "cSpell.words": [
    "brandmark",
    "cutover",
    "dryrun",
    "Eleventy",
    "fitty",
    "HSTS",
    "Intune",
    "leadingzeros",
    "linkify",
    "middlewares",
    "oldoffset",
    "paren",
    "parens",
    "passwordless",
    "prefs",
    "preload",
    "samesite",
    "Sangoma",
    "Shortlink",
    "Tectic",
    "templating",
    "timesheet",
    "twelvehour",
    "Unifi"
  ],
  "editor.defaultFormatter": "Glavin001.unibeautify-vscode",
  "editor.formatOnSave": true,
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "formatFiles.excludedFolders": [
    "node_modules", ".git", "_site"
  ],
  "formatFiles.excludePattern": "*(package-lock.json|headers.liquid|sitemap.liquid)",
  "formatFiles.useGitIgnore": true,
  "[ignore]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "editor.tabSize": 2,
  "markdownlint.config": {
    "default": true,
    "MD026": {
      "punctuation": ".,;:!。，；：" //TODO make this rule work. See GoogleWorkspace.md.
    }
  }
}